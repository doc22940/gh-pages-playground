  },
  "dependencies": {
    "@zeit/next-typescript": "^1.1.1",
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-define": "^1.3.1",
    "styled-components": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rheaditi/gh-pages-playground.git"
  }
}
